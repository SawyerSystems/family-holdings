{
  "name": "family-holdings",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5273",
    "dev:clean": "npm run kill:all && npm run dev",
    "dev:fresh": "npm run clean && npm run dev:clean",
    "dev:full": "npm run kill:all && concurrently \"npm run backend:dev\" \"npm run dev\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "dev:full:clean": "npm run clean && npm run dev:full",
    "start": "npm run dev:full",
    "build": "npm run type-check && vite build",
    "build:clean": "npm run clean && npm run build",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "preview": "vite preview --port 5273",
    "preview:build": "npm run build && npm run preview",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:css": "stylelint \"src/**/*.css\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "clean": "npm run clean:dist && npm run clean:deps && npm run clean:cache",
    "clean:dist": "rimraf dist build",
    "clean:deps": "rimraf node_modules",
    "clean:cache": "rimraf .vite .turbo node_modules/.cache",
    "clean:all": "npm run clean && npm run backend:clean",
    "kill:ports": "npx kill-port 5273 8000 || true",
    "kill:processes": "bash -c \"lsof -ti:5273,8000 | xargs -r kill -9\" || true",
    "kill:all": "npm run kill:ports && npm run kill:processes",
    "backend:setup": "cd backend && python -m venv venv && . venv/bin/activate && pip install -r requirements.txt",
    "backend:dev": "cd backend && . venv/bin/activate && UVICORN_MOCK_AUTH=1 python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "backend:dev:debug": "cd backend && . venv/bin/activate && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000 --log-level debug",
    "backend:test": "cd backend && . venv/bin/activate && python -m pytest",
    "backend:test:coverage": "cd backend && . venv/bin/activate && python -m pytest --cov=. --cov-report=html",
    "backend:clean": "cd backend && rm -rf __pycache__ .pytest_cache .coverage htmlcov && find . -name \"*.pyc\" -delete",
    "backend:requirements": "cd backend && . venv/bin/activate && pip freeze > requirements.txt",
    "backend:check": "cd backend && . venv/bin/activate && python -m flake8 . && python -m mypy .",
    "python:setup": "./setup-python.sh",
    "python:check": "cd backend && . venv/bin/activate && python --version && pip list",
    "db:migrate": "cd backend && . venv/bin/activate && python migrate_db.py",
    "db:test": "cd backend && . venv/bin/activate && python test_db.py",
    "db:reset": "npm run supabase:reset",
    "mobile:sync": "cap sync",
    "mobile:build": "npm run build && cap sync",
    "mobile:ios": "cap open ios",
    "mobile:android": "cap open android",
    "mobile:serve": "cap serve",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:generate-types": "supabase gen types typescript --local > src/types/supabase.ts",
    "supabase:status": "supabase status",
    "setup": "chmod +x setup-dev.sh && ./setup-dev.sh",
    "setup:full": "npm install && npm run backend:setup && npm run python:setup",
    "setup:check": "npm run python:check && npm run type-check && npm run lint",
    "ci": "npm run lint && npm run type-check && npm run test:run && npm run build",
    "ci:coverage": "npm run lint && npm run type-check && npm run test:coverage && npm run build",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:audit": "npm audit",
    "deps:audit:fix": "npm audit fix",
    "docs:dev": "echo 'Documentation server would start here'",
    "docs:build": "echo 'Documentation would be built here'",
    "health:check": "curl -f http://localhost:8000/health && curl -f http://localhost:5273",
    "health:backend": "curl -f http://localhost:8000/health",
  "health:frontend": "curl -f http://localhost:5273",
  "dev:mobile": "concurrently \"npm run backend:dev\" \"vite --host 0.0.0.0 --port 5273\""
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
